# My mail account

set imap_user        = 'name@mail.com'
set imap_pass        = `spm show mymailpass`
set smtp_url         = 'smtps://'
set smtp_pass        = `spm show mymailpass`
set from             = 'name@mail.com'
set realname         = 'Name'
set folder           = 'imaps://'
set spoolfile        = '+INBOX'
set record           = '+Sent' # unset record (gmail)
set postponed        = '+Drafts' # unset postponed  (like above)
