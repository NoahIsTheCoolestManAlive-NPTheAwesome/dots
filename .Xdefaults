Xcursor.theme: Vanilla-DMZ-AA

#include "/home/luke/.colors/blackburn"

Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.dpi: 96
Xft.rgba: rgb

*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48,35:48,58:48,63:48,61:48,44:48,38:48,59:48

*.font: -*-tamsyn-medium-*-*-*-14-*-*-*-*-*-*-* 
*.boldFont: -*-tamsyn-medium-*-*-*-14-*-*-*-*-*-*-* 
*.italicFont: -*-tamsyn-medium-*-*-*-14-*-*-*-*-*-*-*
*.boldItalicFont: -*-tamsyn-medium-*-*-*-14-*-*-*-*-*-*-*

*saveLines: 8192
*scrollBar: false
*dynamicColors: true
*loginShell: true
*multiScroll: true

URxvt.depth: 32
URxvt.background: [95]#020202

URxvt.perl-ext-common:  default,matcher
URxvt.url-launcher:     /usr/bin/dwb
URxvt.matcher.button:   1
